[
    {
      "name": "frontend",
      "links": [
        "mattapi"
      ],
      "image": "tonnu/ws_web",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5001,
          "hostPort": 80
        }
      ],
      "memory": 500,
      "cpu": 10
    },
    {
      "name": "mattapi",
      "image": "tonnu/mattapi",
      "cpu": 10,
      "memory": 500,
      "essential": true,
      "links": [ "redis" ],
      "command": ["python", "/pipeline/source/app.py"]
    },
    {
      "name": "redis",
      "image": "redis",
      "cpu": 10,
      "memory": 500,
      "essential": true
    }
]
